{
  "version": "0.0.1",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "scripts": {
    "start": "yarn serve",
    "build": "yarn build-ts",
    "serve": "node dist/index.js",
    "watch-node": "nodemon --inspect dist/index.js",
    "watch": "concurrently -k -p \"[{name}]\" -n \"TypeScript,Node\" -c \"cyan.bold,green.bold\" \"yarn watch-ts\" \"yarn watch-node\"",
    "build-ts": "tsc",
    "watch-ts": "tsc -w"
  },
  "dependencies": {
    "aws-sdk": "^2.678.0",
    "discord.js": "^12.2.0",
    "dotenv": "^8.0.0",
    "node-fetch": "^1.1.0",
    "source-map-support": "^0.5.12",
    "tmi.js": "^1.5.0",
    "twitch-webhook": "^1.2.4",
    "twitter": "^1.7.1",
    "yup": "^0.27.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/dotenv": "^6.1.1",
    "@types/node": "^12.0.6",
    "@types/tmi.js": "^1.4.0",
    "@types/twitter": "^1.7.0",
    "@types/yup": "^0.26.14",
    "concurrently": "^4.1.0",
    "husky": "^2.4.0",
    "nodemon": "^1.19.4",
    "prettier": "^1.17.1",
    "pretty-quick": "^1.11.0",
    "ramda": "^0.26.1",
    "typescript": "^3.9.2",
    "yarn": "^1.22.4"
  }
}
